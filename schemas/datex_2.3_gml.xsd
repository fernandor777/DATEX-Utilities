<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.vegvesen.no/datex/1.0" version="2.3" xmlns:D2LogicalModel="http://www.vegvesen.no/datex/1.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
  <xs:complexType name="ExternalReferencingType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>A location defined by reference to an external/other referencing system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="externalLocationCode" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>A code in the external referencing system which defines the location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="externalReferencingSystem" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>Identification of the external/other location referencing system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="externalReferencingExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ExternalReferencingPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:ExternalReferencing"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="ExternalReferencing" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:ExternalReferencingType"/>
  <xs:complexType name="GroupOfLocationsLinearType">
    <xs:complexContent>
      <xs:extension base="D2LogicalModel:GroupOfLocations">
        <xs:sequence>
          <xs:element minOccurs="0" name="linearExtension" type="D2LogicalModel:_LinearExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GroupOfLocationsLinearPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:GroupOfLocationsLinear"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="GroupOfLocationsLinear" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:GroupOfLocationsLinearType"/>
  <xs:complexType name="CctvStillImageServiceType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Details of a still image service available from the CCTV camera.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvStillImageServiceLevel" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The level assigned to this still image service.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvStillImageServiceLevelName" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>The name assigned to this still image service level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvStillImageFormat" type="D2LogicalModel:CctvStillImageFormatEnum">
            <xs:annotation>
              <xs:documentation>Still image format (e.g. jpeg).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvStillImageWidth" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>Still image width in pixels.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvStillImageHeight" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>Still image height in pixels.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvStillImageZoomBlankingLevel" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The zoom level beyond which still images for this service level will be blanked.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="stillImageUrl" type="D2LogicalModel:UrlLinkPropertyType">
            <xs:annotation>
              <xs:documentation>The URL from where the current still images from the CCTV camera can be obtained.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="orientationImageUrl" type="D2LogicalModel:UrlLinkPropertyType">
            <xs:annotation>
              <xs:documentation>The URL from where a sample orientation image for the still image service from the CCTV camera can be obtained to assist in identifying camera orientation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="thumbnailImageUrl" type="D2LogicalModel:UrlLinkPropertyType">
            <xs:annotation>
              <xs:documentation>Thumbnail image URL</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cctvStillImageServiceExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CctvStillImageServicePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:CctvStillImageService"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="CctvStillImageService" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:CctvStillImageServiceType"/>
  <xs:complexType name="CctvCameraNonDwellZoneType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>A viewing zone in which the CCTV camera is not allowed to stop,identified by a virtual rectangular area defined by a top left and bottom right position. Normally the camera is automatically moved through this zone expeditiously.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvNonDwellTopLeftPanValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The pan value for the CCTV camera at the top left of the dwell zone.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvNonDwellTopLeftTiltValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The tilt value for the CCTV camera at the top left of the dwell zone.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvNonDwellBottomRightPanValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The pan value for the CCTV camera at the bottom right of the dwell zone.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvNonDwellBottomRightTiltValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The tilt value for the CCTV camera at the bottom right of the dwell zone.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cctvCameraNonDwellZoneExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CctvCameraNonDwellZonePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:CctvCameraNonDwellZone"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="CctvCameraNonDwellZone" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:CctvCameraNonDwellZoneType"/>
  <xs:complexType name="OpenlrLineAttributesType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Line attributes are part of a location reference point and consists of functional road class (FRC),form of way (FOW) and bearing (BEAR) data.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="openlrFunctionalRoadClass" type="D2LogicalModel:OpenlrFunctionalRoadClassEnum">
            <xs:annotation>
              <xs:documentation>The functional road class (FRC) can hold eight different values as described in the logical format.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="openlrFormOfWay" type="D2LogicalModel:OpenlrFormOfWayEnum">
            <xs:annotation>
              <xs:documentation>The form of way (FOW) can hold eight different values as described in the logical format.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="openlrBearing" type="D2LogicalModel:AngleInDegrees">
            <xs:annotation>
              <xs:documentation>defines the bearing field as an integer value between 0 and 360 whereby “0” is included and “360” is excluded from that range.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="openlrLineAttributesExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OpenlrLineAttributesPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:OpenlrLineAttributes"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="OpenlrLineAttributes" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:OpenlrLineAttributesType"/>
  <xs:complexType name="_PeriodExtensionTypeType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:sequence>
          <xs:element minOccurs="0" name="periodExtended" type="D2LogicalModel:PeriodExtendedPropertyType"/>
          <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="_PeriodExtensionTypePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:_PeriodExtensionType"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="_PeriodExtensionType" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:_PeriodExtensionTypeType"/>
  <xs:complexType name="PublicHolidayType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Specification of the public holiday type in a specific country or region. Use this component only when specialDayType is set to 'publicHoliday' or 'holidays'.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="country" type="D2LogicalModel:CountryEnum">
            <xs:annotation>
              <xs:documentation>ISO 3166-1 two character country code.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="countrySubdivision" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>ISO 3166-2 country sub-division code (up to 3 characters).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="region" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>Region of country (e.g. &quot;Scotland&quot;, &quot;Wales&quot; etc. if country = GB) </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="publicHolidayType" type="D2LogicalModel:PublicHolidayTypeEnum">
            <xs:annotation>
              <xs:documentation>Specifies the public holiday type for the country or region.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="publicHolidayName" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>Specification of public holiday, if the enumeration values do not fit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="publicHolidayExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PublicHolidayPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:PublicHoliday"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="PublicHoliday" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:PublicHolidayType"/>
  <xs:complexType name="CctvCameraControlLimitsType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Limits imposed on the control of the CCTV camera.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvEndStopMinPanValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The pan value for the CCTV camera at its minimum pan end stop. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvEndStopMaxPanValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The pan value for the CCTV camera at its maximum pan end stop. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvEndStopMinTiltValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The tilt value for the CCTV camera at its minimum tilt end stop. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvEndStopMaxTiltValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The tilt value for the CCTV camera at its maximum tilt end stop. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvMaxZoomValue" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>The zoom value for the CCTV camera at its maximum zoom setting. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="cctvCameraNonDwellZone" type="D2LogicalModel:CctvCameraNonDwellZonePropertyType"/>
          <xs:element minOccurs="0" name="cctvCameraControlLimitsExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CctvCameraControlLimitsPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:CctvCameraControlLimits"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="CctvCameraControlLimits" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:CctvCameraControlLimitsType"/>
  <xs:complexType name="PointCoordinatesType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>A pair of coordinates defining the geodetic position of a single point using the European Terrestrial Reference System 1989 (ETRS89).</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="latitude" type="D2LogicalModel:Float">
            <xs:annotation>
              <xs:documentation>Latitude in decimal degrees using the European Terrestrial Reference System 1989 (ETRS89).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="longitude" type="D2LogicalModel:Float">
            <xs:annotation>
              <xs:documentation>Longitude in decimal degrees using the European Terrestrial Reference System 1989 (ETRS89).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="pointCoordinatesExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PointCoordinatesPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:PointCoordinates"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="PointCoordinates" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:PointCoordinatesType"/>
  <xs:complexType name="SpecialDayType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Specification of a special day, for example schoolDay, electionDay, ...  Gives also the possibility to define a public holiday (country specific).</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="intersectWithApplicableDays" type="D2LogicalModel:Boolean">
            <xs:annotation>
              <xs:documentation>When true, the period is the intersection of applicable days and this special day. When false, the period is the union of applicable days and this special day.”</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="specialDayType" type="D2LogicalModel:SpecialDayTypeEnum">
            <xs:annotation>
              <xs:documentation>Specification of a special day, for example schoolDay, electionDay, ..  .</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="specialDayName" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>Specification of a special day, if the enumeration values do not fit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="publicHoliday" type="D2LogicalModel:PublicHolidayPropertyType"/>
          <xs:element minOccurs="0" name="specialDayExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SpecialDayPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:SpecialDay"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="SpecialDay" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:SpecialDayType"/>
  <xs:complexType name="CauseType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Contains details of the cause of a record within a situation</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="causeExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CausePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:Cause"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="Cause" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:CauseType"/>
  <xs:complexType name="FilterExitManagementType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Filter indicators management information.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="filterEnd" type="D2LogicalModel:Boolean">
            <xs:annotation>
              <xs:documentation>This attribute, set to true, indicates that the filter, for which a previous record version has been published, becomes inactive.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="filterOutOfRange" type="D2LogicalModel:Boolean">
            <xs:annotation>
              <xs:documentation>This attribute is set to true when a previous version of this record has been published and now, for this new record version, the record goes out of the filter range.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="filterExitManagementExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FilterExitManagementPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:FilterExitManagement"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="FilterExitManagement" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:FilterExitManagementType"/>
  <xs:complexType name="HeaderInformationType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Management information relating to the data contained within a publication.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="areaOfInterest" type="D2LogicalModel:AreaOfInterestEnum">
            <xs:annotation>
              <xs:documentation>The extent of the geographic area to which the related information should be distributed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="confidentiality" type="D2LogicalModel:ConfidentialityValueEnum">
            <xs:annotation>
              <xs:documentation>The extent to which the related information may be circulated, according to the recipient type. Recipients must comply with this confidentiality statement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="informationStatus" type="D2LogicalModel:InformationStatusEnum">
            <xs:annotation>
              <xs:documentation>The status of the related information (real, test, exercise ....).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="urgency" type="D2LogicalModel:UrgencyEnum">
            <xs:annotation>
              <xs:documentation>This indicates the urgency with which a message recipient or Client should distribute the enclosed information.  Urgency particularly relates to functions within RDS-TMC applications.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="headerInformationExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HeaderInformationPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:HeaderInformation"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="HeaderInformation" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:HeaderInformationType"/>
  <xs:complexType name="LocationExtensionType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>Extension of the Location class</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="roadNumber" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>Identifier/number of the road of which the linear element forms a part.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="countyNumber" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>County number</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="municipalityNumber" type="D2LogicalModel:NonNegativeInteger">
            <xs:annotation>
              <xs:documentation>Municipality number</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LocationExtensionPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="D2LogicalModel:LocationExtension"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
  </xs:complexType>
  <xs:element name="LocationExtension" substitutionGroup="gml:AbstractFeature" type="D2LogicalModel:LocationExtensionType"/>
  <xs:complexType name="ImpactType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>An assessment of the impact that an event or operator action defined by the situation record has on the driving conditions.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="capacityRemaining" type="D2LogicalModel:Percentage">
            <xs:annotation>
