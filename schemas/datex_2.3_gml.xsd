  <xs:complexType name="CctvCameraMetadataRecordType">
    <xs:complexContent>
      <xs:extension base="gml:AbstractFeatureType">
        <xs:annotation>
          <xs:documentation>A record containing the metadata relating to a specific CCTV camera.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvCameraIdentification" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>The identifier assigned to the CCTV camera at its deployed location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvCameraSerialNumber" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>The serial number of the actual CCTV camera equipment installed. When the deployed camera is replaced (e.g. during maintenance) this serial number will change, whereas the camera identification will stay the same.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvCameraRecordVersionTime" type="D2LogicalModel:DateTime">
            <xs:annotation>
              <xs:documentation>The date/time that this version of the CCTV camera record was defined. The identity and version of the CCTV camera record are defined by the class stereotype implementation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvCameraSiteLocalDescription" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>A description of the site where the CCTV camera is deployed which is likely to be used by users with local knowledge (e.g. &quot;M4 187/8b St Catherine's Hill&quot;).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvCameraSiteNationalDescription" type="D2LogicalModel:MultilingualStringPropertyType">
            <xs:annotation>
              <xs:documentation>A description of the site where the CCTV camera is deployed which is likely to be used by users without local knowledge (e.g. &quot;M4 westbound between J20 and J21&quot;).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="cctvCameraOrientationDescription" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>An image content description which may help clients identify camera orientation, e.g. &quot;The carriageway closest to the camera is northbound&quot;, or &quot;If the road curves to the left the camera is pointing south&quot;.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="cctvCameraType" type="D2LogicalModel:CctvCameraTypeEnum">
            <xs:annotation>
              <xs:documentation>The type of the CCTV camera.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvCameraHeightAboveRoadway" type="D2LogicalModel:MetresAsFloat">
            <xs:annotation>
              <xs:documentation>The height in metres of the CCTV camera head above the roadway.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvFixedCamera" type="D2LogicalModel:Boolean">
            <xs:annotation>
              <xs:documentation>An indication of whether the CCTV camera is a fixed type or a pan/tilt/zoom type. &quot;True&quot; indicates it is fixed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="cctvStaticCamera" type="D2LogicalModel:Boolean">
            <xs:annotation>
              <xs:documentation>An indication of whether the CCTV camera is a static or a mobile type. &quot;True&quot; indicates it is static.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="nextCctvCamera" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>The identifier of the next CCTV camera on the same road relative to the traffic direction on the principle monitored carriageway (there may be multiple next cameras at junctions and slip roads).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="previousCctvCamera" type="D2LogicalModel:String">
            <xs:annotation>
              <xs:documentation>The identifier of the previous CCTV camera on the same road relative to the traffic direction on the principle monitored carriageway (there may be multiple previous cameras at junctions and slip roads).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="source" type="D2LogicalModel:SourcePropertyType"/>
          <xs:element name="cctvCameraLocation" type="D2LogicalModel:LocationPropertyType">
            <xs:annotation>
              <xs:documentation>Location of CCTV camera.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="cctvPotentialMonitoringLocation" type="D2LogicalModel:LocationPropertyType">
            <xs:annotation>
              <xs:documentation>A location (i.e. a point such as a car park exit, a linear section of the road network with specified direction or an area) that the CCTV camera is potentially capable of effectively monitoring.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="primaryConrolResponsibility" type="D2LogicalModel:PrimaryConrolResponsibilityPropertyType"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="cctvStillImageService" type="D2LogicalModel:CctvStillImageServicePropertyType"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="cctvVideoService" type="D2LogicalModel:CctvVideoServicePropertyType"/>
          <xs:element minOccurs="0" name="cctvCameraControlData" type="D2LogicalModel:CctvCameraControlDataPropertyType"/>
          <xs:element minOccurs="0" name="cctvCameraMetadataRecordExtension" type="D2LogicalModel:_ExtensionTypePropertyType"/>
          <xs:element minOccurs="0" name="id" type="xs:string"/>
          <xs:element minOccurs="0" name="version" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
